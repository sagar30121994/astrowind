---
import favIcon from '~/assets/favicons/favicon.jpg';
import favIconSvg from '~/assets/favicons/favicon.jpg';
import appleTouchIcon from '~/assets/favicons/apple-touch-icon.jpg';
---

<link rel="shortcut icon"  href={favIcon.src} />
<link rel="icon" type="image/jpg" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
